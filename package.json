  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/deasync": "^0.1.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "typescript": "^4.8.4",
    "version-bump": "^0.1.0"
